use lib Std
use lib Std::Console
use lib Std::System

class Printer:
    string Text()
    
    fn __Init(string Text):
        this.Text = Text
    esc

    fn Print() => WriteLn(this.Text)
esc

fn Main({} of <string> argv):
    var Printer HelloWorld("Hello World!")
    HelloWorld.Print()
esc
