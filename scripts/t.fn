
in Console import putln # Not println because putln is faster

fun Main() {
    start: float = System.timeSinceYear(2025)
    x: int = 0

    for i in 1'000'000 {
        putln(x += i)
    }
    
    finish: float = System.timeSinceYear(2025)
    Console.println("time: \{finish - start}\nresult: \{x}")
}
